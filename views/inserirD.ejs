<%- include("./head.ejs"); %>
 <div>  
 <div class="card-header">      
 <h4>Registro da tabela mestre</h4>  
 <div class="form-group">     
 <label>IdQuestao</label>    
 <input type="text" class="form-control form-control-sm" name="IdQuestao" value="<%= dado.IdQuestao %>" readonly="readonly" >    
 </div>   
 <div class="form-group">     
 <label>Descricao</label>    
 <input type="text" class="form-control form-control-sm" name="Descricao" value="<%= dado.Descricao %>" readonly="readonly" >    
 </div>   
 <div class="form-group">     
 <label>Resposta_correta</label>    
 <input type="text" class="form-control form-control-sm" name="Resposta_correta" value="<%= dado.Resposta_correta %>" readonly="readonly" >    
 </div>   
 <div class="form-group">     
 <label>IdPessoa_FK</label>    
 <input type="text" class="form-control form-control-sm" name="IdPessoa_FK" value="<%= dado.IdPessoa_FK %>" readonly="readonly" >    
 </div>   
 <div class="form-group">     
 <label>IdProcedimento_FK</label>    
 <input type="text" class="form-control form-control-sm" name="IdProcedimento_FK" value="<%= dado.IdProcedimento_FK %>" readonly="readonly" >    
 </div>   
 </div>     
 </div>    
 <!-- Atualizar dados da tabela Publicacao -->  
 <div class="card-header">  
 <ul class="nav nav-pills w-100">     
 <li class="nav-pill active">   
 <h4>Incluir na Tabela de Detalhes</h4>     
 </li>     
 </ul>     
 <div>  
 <!-- Form para inclusao de nova regisgro Detalhes para Mestre -->   
 <div class="card-body">   
 <!-- <form  action="/inserirDetalhe" method="post"> -->   
 <form  action="/inserirD/<%= dado.IdQuestao %>/<%= dado.IdResposta %>" method="post">  
 <table class="table table-striped table-hover table-active">   
 <tr>    
 <td colspan="2">Detalhes relaionado com a tabela Mestre</td>   
 </tr>   
 <tr>    
 <td>Letra</td>   
 <td><input class="form-control" type="text" name="Letra" value="<%= Letra %>" /></td>  
 </tr>   
 <tr>    
 <td>DescricaoResposta</td>   
 <td><input class="form-control" type="text" name="DescricaoResposta" value="<%= DescricaoResposta %>" /></td>  
 </tr>   
 <tr>    
 <td colspan="2">    
 <input class="btn btn-primary" type="submit" name="btIncuirPub" value="Inserir"/>    
 <a href="/" class="btn btn-outline-primary btn-sm">Voltar</a>      
 </td>    
 </tr>   
 </table>   
 </form>   
 </div>      
 <footer>  
 <p>&copy; 2020 </p>  
 </footer>  
 </body>     
 </html>  